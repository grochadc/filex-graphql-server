{
  "name": "filex-graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "npx tsc",
    "dev": "nodemon --ext ts,tsx --exec 'ts-node --files src/server.ts'",
    "codegenserver": "ts-node -T --files src/server.ts",
    "publish": "git push heroku master",
    "postpublish": "heroku logs --tail",
    "deploy": "git push heroku master",
    "postdeploy": "heroku logs --tail",
    "database": "heroku config:get DATABASE_URL | xargs pgweb --url",
    "codegen": "graphql-codegen --config codegen.yml",
    "databaseURL": "heroku config:get HEROKU_POSTGRESQL_TEAL_URL -a filex-database"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.2.1",
    "@types/gapi": "^0.0.39",
    "@types/jest": "^29.0.0",
    "@types/node": "^14.11.2",
    "apollo-datasource-firebase": "^0.0.43",
    "apollo-datasource-rest": "^0.9.4",
    "apollo-server": "^2.18.1",
    "apollo-server-testing": "^2.19.2",
    "babel-core": "^6.26.3",
    "babel-plugin-import-graphql": "^2.8.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "firebase": "^7.21.1",
    "googleapis": "39",
    "graphql": "^15.5.3",
    "node-fetch": "^2.6.1",
    "pg-promise": "^10.11.0",
    "ramda": "^0.27.1",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "2.1.0",
    "@graphql-codegen/typescript-resolvers": "2.1.0",
    "@graphql-codegen/visitor-plugin-common": "^2.1.0",
    "babel-jest": "^26.6.3",
    "jest": "^29.0.2",
    "jest-mock-extended": "^2.0.7",
    "mockdate": "^3.0.2",
    "node-ts": "^5.1.2",
    "nodemon": "^2.0.4",
    "prisma": "^4.1.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1"
  }
}
